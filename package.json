{
  "name": "ts-boilerplate-lib",
  "version": "1.0.0",
  "description": "Boilerplate for TS library",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist && rm -rf node_modules",
    "start": "tsc && node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "doc": "typedoc",
    "example": "tsc && node dist/examples/example.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.2"
  },
  "dependencies": {}
}
